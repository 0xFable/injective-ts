{
  "name": "@injectivelabs/wallet-ts",
  "description": "A convenient way to use different types of wallets on Injective.",
  "version": "1.0.340-alpha.0",
  "author": {
    "name": "Bojan Angjelkoski",
    "email": "bojan@injectivelabs.org"
  },
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "~wallet-ts": "dist"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "postinstall": "shx mkdir -p dist && link-module-alias",
    "build": "tsc --build tsconfig.build.json && tsc --build tsconfig.build.esm.json && yarn build:post && link-module-alias",
    "build:watch": "tsc --build -w tsconfig.build.json && tsc -w --build tsconfig.build.esm.json && yarn build:post && link-module-alias",
    "build:post": "shx cp ../../stub/package.json.stub dist/cjs/package.json && shx cp ../../stub/package.esm.json.stub dist/esm/package.json",
    "clean": "tsc --build tsconfig.build.json --clean && tsc --build tsconfig.build.esm.json --clean && shx rm -rf coverage *.log junit.xml dist && jest --clearCache",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/unit",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --reporters='jest-junit'",
    "coverage": "jest --coverage",
    "coverage:show": "live-server coverage",
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@alch/alchemy-web3": "1.4.7",
    "@cosmjs/launchpad": "0.27.1",
    "@cosmjs/proto-signing": "0.29.4",
    "@cosmostation/extension-client": "^0.1.11-alpha.0",
    "@ethereumjs/common": "^2.6.3",
    "@ethereumjs/tx": "^3.5.1",
    "@injectivelabs/chain-api": "1.9.6",
    "@injectivelabs/contracts": "^1.0.97",
    "@injectivelabs/exceptions": "^1.0.50",
    "@injectivelabs/networks": "^1.0.81",
    "@injectivelabs/sdk-ts": "^1.0.379",
    "@injectivelabs/token-metadata": "^1.0.119",
    "@injectivelabs/ts-types": "^1.0.28",
    "@injectivelabs/utils": "^1.0.69",
    "@keplr-wallet/cosmos": "^0.11.17",
    "@keplr-wallet/types": "^0.11.17",
    "@ledgerhq/hw-app-eth": "6.27.1",
    "@ledgerhq/hw-transport": "6.27.1",
    "@ledgerhq/hw-transport-u2f": "^5.36.0-deprecated",
    "@ledgerhq/hw-transport-webhid": "6.27.1",
    "@ledgerhq/hw-transport-webusb": "6.27.1",
    "@metamask/eth-sig-util": "^4.0.0",
    "@solana/wallet-adapter-wallets": "^0.19.5",
    "@terra-money/terra.js": "3.0.9",
    "@terra-money/wallet-controller": "^3.9.4",
    "@toruslabs/torus-embed": "1.22.1",
    "@types/eth-sig-util": "^2.1.1",
    "@types/ethereumjs-util": "^6.1.0",
    "@types/hdkey": "^2.0.1",
    "@types/ledgerhq__hw-transport-webusb": "^4.70.1",
    "@walletconnect/web3-provider": "^1.7.8",
    "eip1193-provider": "^1.0.1",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-util": "^7.1.0",
    "ethers": "^5.6.8",
    "hdkey": "^2.0.1",
    "link-module-alias": "^1.2.0",
    "rxjs": "^7.5.5",
    "shx": "^0.3.3",
    "trezor-connect": "^8.2.8",
    "web3": "^1.7.3",
    "web3-core": "^1.7.3"
  },
  "gitHead": "00176548701ceed002a45fd690bac43653b88d60",
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "wallet-ts API Documentation"
  }
}
